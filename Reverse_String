public String reverseString(String s) {
    String temp = s;
    char[] ch = s.toCharArray();

    int begin = 0;
    int end = ch.length - 1;
    
    while (begin <= end) {
        char temp = ch[begin];
        ch_copy[begin] = ch_copy[end];
        ch_copy[end] = temp;
        begin++;
        end--;
    }
    return new String (ch_copy);
}
