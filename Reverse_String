#7/27/2016
public String reverseString(String s) {
    String temp = s;
    char[] ch = s.toCharArray();
    char[] ch_copy = s.toCharArray();

    int begin = 0;
    int end = ch.length - 1;
    
    while (begin <= end) {
        ch_copy[begin] = ch_copy[end];
        ch_copy[end] = ch[begin];
        begin++;
        end--;
    }
    return String.valueOf(ch_copy);
}
